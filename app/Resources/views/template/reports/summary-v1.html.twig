{% extends "main.html.twig" %}
{% block sidebar_close %}page-sidebar-closed{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div id="page-container">
                {{ include('tpl/_loading.html.twig')}}
            </div>
        </div>
    </div
{% endblock %}

{% block isReact %}{% endblock %}
{% block javascripts %}
    {{ parent() }}
  
    <script type="text/javascript">
        window.hostIp = "{{ hostIp }}";
    </script>

    {% if app.environment == "prod" %}
        <script type="text/javascript" src="{{ asset('compiled/js/summary-v1.react.min.js', 'compiled') }}"></script>
    {% else %}
        <script type="text/babel" src="{{ asset('scripts/summary-v1/summary-v1-all-table.react.js', 'pages') }}"></script>
        <script type="text/babel" src="{{ asset('scripts/summary-v1/summary-v1-table.react.js', 'pages') }}"></script>
        <script type="text/babel" src="{{ asset('scripts/summary-v1/summary-v1.react.js', 'pages') }}"></script>
    {% endif %}

{% endblock %}